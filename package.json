{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:css": "stylelint src/**/*.scss",
    "lint:css:staged": "stylelint --fix",
    "lint:fix": "npm run lint:js && npm run lint:tsx && npm run lint:css -- --fix",
    "lint:js": "eslint --fix --ext .js ./",
    "lint:ts": "eslint --fix --ext .ts ./",
    "lint:tsx": "eslint --fix --ext .tsx ./"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.22.2",
    "@hookform/resolvers": "^2.8.5",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.25.0",
    "clsx": "^1.1.1",
    "macro-css": "^1.0.5",
    "next": "11.1.0",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.22.5",
    "sass": "^1.38.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "^2.5.1",
    "prettier-eslint": "^10.1.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "npm run lint:js"
    ],
    "*.ts": [
      "prettier --write",
      "npm run lint:ts"
    ],
    "*.tsx": [
      "prettier --write",
      "npm run lint:tsx"
    ]
  },
  "stylelint": {
    "extends": "./linters/stylelintrc.json"
  }
}
